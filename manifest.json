{
    "applications": {
      "gecko": {
        "strict_min_version": "58.0a1"
      }
    },
    "browser_specific_settings": {
      "gecko": {
        "id": "ProTabs@protabs.com",
        "strict_min_version": "42.0"
      }
    },
    "icons": {
        "32": "icons/tabs-32.png",
        "48": "icons/tabs-48.png",
        "96": "icons/tabs-96.png"
    },
    "browser_action": {
      "browser_style": true,
      "default_icon": "icons/tabs-32.png",
      "default_title": "ProTabs",
      "default_popup": "popup/tabs.html"
    },
    "content_scripts": [
      {
        "matches": "/listview/listview.html",
        "js": ["/listview/listview.js"]
      }
    ],
    "description": "Manage and restore sessions easily via list or browser.",
    "manifest_version": 2,
    "name": "ProTabs",
    "permissions": [
      "tabs",
      "storage"
    ],
    "version": "1.0"
  }